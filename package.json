{
  "name": "ds-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "demo": "webpack-dev-server --open --config build/webpack.demo.conf.js",
    "lib": "yarn lib:style&&yarn lib:js",
    "doc:serve": "webpack-dev-server --open --config build/webpack.doc.dev.conf.js",
    "lib:style": "gulp --gulpfile build/build-style.js",
    "lib:js": "webpack --config build/webpack.lib.conf.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^lib@/(.*)$": "<rootDir>/src/$1"
    },
    "coverageDirectory": "<rootDir>/test/coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/componets/*.{js,vue}",
      "src/directives/*.js",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "author": "",
  "license": "ISC",
